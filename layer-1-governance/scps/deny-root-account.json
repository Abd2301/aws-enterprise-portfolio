{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "DenyRootAccountActions",
        "Effect": "Deny",
        "Action": "*",
        "Resource": "*",
        "Condition": {
          "StringLike": {
            "aws:PrincipalArn": "arn:aws:iam::*:root"
          }
        }
      }
    ]
  }